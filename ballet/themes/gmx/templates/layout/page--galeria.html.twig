<div id="page-wrapper">
  <div id="page">


  <!--Start: Branding and Menu -->
<div id="stick">
  <div class="menu">
    <div class="container">
      <div class="row">
       <div class="branding">
        {% if page.header %}
          <div class="col-xs-6 col-md-3">
            {{ page.header }}
          </div>
        {% endif %}
        <!-- Navigation -->
        {% if page.menu %}
          <div class="col-xs-6 col-md-9">
            {{ page.menu }} 
          </div>
        {% endif %}
        <!--End Navigation -->
       </div>
      </div>
    </div>
  </div>
</div>

      <!--- Start content -->
      {% if page.content %}
        <div class="container-fluid contenido ">
          <div class="row">
            <div class="container">
              <div class="col-md-12">
                <div class="contenidorow">
              {{ page.content }}<br />
              {{ page.galerianodo }}
                </div>
              </div>
            </div>
          </div>
        </div>
      {% endif %}
      <!---End content -->

      <!--- Start Equipo -->
      {% if page.equipo %}
        <div class="container-fluid equipo ">
          <div class="row">
            <div class="container">
              <div class="col-md-12">
                <div class="equiporow">
                 {{ page.balleta }}
                </div>
              </div>
            </div>
          </div>
        </div>
      {% endif %}
      <!---End Equipo -->


<!--Start: Footer -->
  <div class="footer">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
        				<ul class="social">
									<li><a href="https://facebook.com/balletconsciente/" class="icon fa-facebook"><span></span></a></li>
									<li><a href="https://www.instagram.com/balletconsciente/" class="icon fa-instagram"><span>Twitter</span></a></li>
								</ul>
          {{ page.footera }}
        </div>
      </div>
    </div>
  </div>
<!--End: Footer -->

  </div>
</div>